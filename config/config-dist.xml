<config>
    <server>
        <ui enabled="no"/>
        <name>__DEFAULT_NAME__</name>
        <udn/>
        <home>__DEFAULT_HOME__</home>
        <webroot>__DEFAULT_WEBROOT__</webroot>
        <storage driver="sqlite3">
            <database-file>sqlite3.db</database-file>
        </storage>
    </server>
    <import>
        <filesystem-charset>ISO-8859-1</filesystem-charset>
        <script>__DEFAULT_SCRIPT__</script>
        <mappings>
            <extension-mimetype ignore-unknown="no">
                <map from="mp3" to="audio/mpeg"/>
                <map from="asf" to="video/x-ms-asf"/>
                <map from="asx" to="video/x-ms-asf"/>
                <map from="wma" to="audio/x-ms-wma"/>
                <map from="wax" to="audio/x-ms-wax"/>
                <map from="wmv" to="video/x-ms-wmv"/>
                <map from="wvx" to="video/x-ms-wvx"/>
                <map from="wm" to="video/x-ms-wm"/>
                <map from="wmx" to="video/x-ms-wmx"/>
                <map from="wmx" to="video/x-ms-wmx"/>
            </extension-mimetype>
            <mimetype-upnpclass>
                <map from="audio/*" to="object.item.audioItem"/>
                <map from="video/*" to="object.item.videoItem"/>
                <map from="image/*" to="object.item.imageItem"/>
            </mimetype-upnpclass>
        </mappings>
        <library-options>
            <libexif>
                <auxdata>
                    <add tag="EXIF_TAG_MODEL"/>
                </auxdata>
            </libexif>
        </library-options>
    </import>
</config>

